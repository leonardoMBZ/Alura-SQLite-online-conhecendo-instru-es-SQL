--ATIVIDADE 1
-- ISERINDO DADOS EM UMA TABELA SQL 

-- CRIAÇÃO DA TABELA
 CREATE TABLE ALUNOS
 (
    ID_ALUNOS INT PRIMARY KEY,
    NOME VARCHAR(20),
    SERIE VARCHAR(5),
    IDADE INT,
    CONTATO INT(12),
    UF CHAR(2)
 );
 
 -- INSERIR DADOS NA TABELA PELO COMANDO INSERT INTOALUNOS
 
 INSERT INTO ALUNOS 
 ( ID_ALUNOS,NOME,SERIE,IDADE, CONTATO,UF) 
 VALUES (1,'LUANA SILVA MARANHAO','8-ANO',16,'71 9 9999-9999','PA');
 
 -- CONSULTANDO OS DADOS INSERIDOS NA ABELA
 
 SELECT * FROM ALUNOS ;
 
 --- COMO INSERIR MAIS DE UM REGISTRO DE ALUNO NA TABELA
 
 
  INSERT INTO ALUNOS 
 ( ID_ALUNOS,NOME,SERIE,IDADE, CONTATO,UF) 
 VALUES 
 (2,'LUANA FRANCA GOMES','8-ANO',13,'71 9 9999-9999','PA'),
 (3,'CARLOS SILVA SOUZA','9-ANO',15,'81 9 9999-9999','BA'),
 (4,'KARLLA ALMEIDA PALMARES','3-ANO-M',18,'81 9 9999-9999','PE');
 -- FAZENDO ISSO OS VALORES VAO SER INSERIDOS DE FORMA CORRETA, OTIMIZANDO O TEMPO DE TRABALHO ALUNOS
 --SEM A NECESSIDADE DE RESCREVER AS LINHAS TODAS NOVAMENTE
 
  SELECT * FROM ALUNOS;
 
